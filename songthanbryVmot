local player = Players.LocalPlayer
local PlaceId = game.PlaceId

local function getHRP()
    local char = player.Character or player.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end

--==================================================
-- TAB MAIN
--==================================================
local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998"
})

Tab:AddButton({
    Name = "üöÄ Teleport ƒêi·ªÉm Radioactive",
    Callback = function()
        local player = game.Players.LocalPlayer
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")

        hrp.CFrame = CFrame.new(896.75, -19.36 + 3, -586.70)
    end
})

Tab:AddToggle({
    Name = "Auto TP Ticket üéüÔ∏è",
    Default = SaveData.AutoTPTT,
    Callback = function(v)
        SaveData.AutoTPTT = v
        Save()
        getgenv().AUTO_TP_TREO = v

        if not v then
            return
        end

        -- ===== SERVICE =====
        local Players = game:GetService("Players")
        local lp = Players.LocalPlayer

        -- ===== CONFIG =====
        local SCAN_DELAY = 0.25 -- 0.25s qu√©t 1 l·∫ßn
        local KEYWORD = "ticket"

        -- ===== FUNC =====
        local function getHRP()
            local char = lp.Character or lp.CharacterAdded:Wait()
            return char:WaitForChild("HumanoidRootPart")
        end

        -- Qu√©t Ticket trong TO√ÄN B·ªò CODE
        local function scanTicket()
            for _, v in ipairs(game:GetDescendants()) do
                if string.find(string.lower(v.Name), KEYWORD) then
                    if v:IsA("BasePart") then
                        return v
                    elseif v:IsA("Model") and v.PrimaryPart then
                        return v.PrimaryPart
                    end
                end
            end
            return nil
        end

        -- ===== LOOP =====
        task.spawn(function()
            local target = nil

            while getgenv().AUTO_TP_TREO do
                if not target then
                    target = scanTicket()
                end

                if target and target.Parent then
                    getHRP().CFrame = target.CFrame + Vector3.new(0, 2, 0)
                else
                    target = nil
                end

                task.wait(SCAN_DELAY)
            end
        end)
    end
})


Tab:AddToggle({
    Name = "Auto TP Treo ‚è∞",
    Default = SaveData.AutoTPT,
    Callback = function(v)
        SaveData.AutoTPT = v
        Save()
        getgenv().AUTO_TP_TREO = v

        if not v then
            return
        end

        -- ===== CONFIG =====
        local Players = game:GetService("Players")
        local TeleportService = game:GetService("TeleportService")
        local lp = Players.LocalPlayer

        local TIMES = 10
        local DELAY = 0.5

        local POS1 = Vector3.new(425.26, -14.16, -340.05)
        local POS2 = Vector3.new(1131.63, 3.11, 531.83)
        local POS3 = Vector3.new(2571.70, -6.74, -336.66)

        -- ===== FUNC =====
        local function getHRP()
            local char = lp.Character or lp.CharacterAdded:Wait()
            return char:WaitForChild("HumanoidRootPart")
        end

        local function teleport(pos)
            getHRP().CFrame = CFrame.new(pos)
        end

        local function spamTP(pos)
            for i = 1, TIMES do
                if not getgenv().AUTO_TP_TREO then return end
                teleport(pos)
                task.wait(DELAY)
            end
        end

        local function inTime()
            local m = tonumber(os.date("%M"))
            return m >= 5 and m < 15
        end

        -- ===== AUTO TREO LOOP =====
        task.spawn(function()
            while getgenv().AUTO_TP_TREO do
                if inTime() then
                    spamTP(POS1)
                    spamTP(POS2)
                    spamTP(POS3)

                    if getgenv().AUTO_TP_TREO then
                        task.wait(1)
                        TeleportService:Teleport(game.PlaceId, lp)
                        break
                    end
                else
                    task.wait(10)
                end
            end
        end)
    end
})

Tab:AddToggle({
    Name = "‚ö° B·∫•m Nhanh",
    Default = SaveData.InstantPick,
    Callback = function(v)
        SaveData.InstantPick = v
        Save()

        if v then
            loadstring([[
                -- HACKBORY | Instant ProximityPrompt
                for _, p in pairs(workspace:GetDescendants()) do
                    if p:IsA("ProximityPrompt") then
                        p.HoldDuration = 0
                        p.RequiresLineOfSight = false
                    end
                end

                workspace.DescendantAdded:Connect(function(p)
                    if p:IsA("ProximityPrompt") then
                        p.HoldDuration = 0
                        p.RequiresLineOfSight = false
                    end
                end)
            ]])()
        end
    end
})

Tab:AddButton({
    Name = "üöÄ Teleport L√™n (M·ªü Kho√° Vip)",
    Callback = function()
        local TweenService = game:GetService("TweenService")
        local Players = game:GetService("Players")

        local plr = Players.LocalPlayer
        local char = plr.Character or plr.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")

        -- ƒêI·ªÇM B·∫ÆT ƒê·∫¶U & K·∫æT TH√öC
        local startPos = Vector3.new(152.34, 3.26, -137.72)
        local endPos   = Vector3.new(1814.02, 3.39, -135.68)

        -- T·ªêC ƒê·ªò
        local speed = 350

        -- ƒê∆ØA NH√ÇN V·∫¨T V·ªÄ ƒêI·ªÇM B·∫ÆT ƒê·∫¶U
        hrp.CFrame = CFrame.new(startPos)

        -- T√çNH TH·ªúI GIAN THEO KHO·∫¢NG C√ÅCH
        local distance = (endPos - startPos).Magnitude
        local time = distance / speed

        -- TWEEN D·ªäCH CHUY·ªÇN
        local tweenInfo = TweenInfo.new(
            time,
            Enum.EasingStyle.Linear,
            Enum.EasingDirection.Out
        )

        local tween = TweenService:Create(
            hrp,
            tweenInfo,
            {CFrame = CFrame.new(endPos)}
        )

        tween:Play()
    end
})

Tab:AddButton({
    Name = "üîΩ Teleport Xu·ªëng (Quay L·∫°i)",
    Callback = function()
        local TweenService = game:GetService("TweenService")
        local Players = game:GetService("Players")

        local plr = Players.LocalPlayer
        local char = plr.Character or plr.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")

        local startPos = Vector3.new(1814.02, 3.39, -135.68)
        local endPos   = Vector3.new(152.34, 3.26, -137.72)
        local speed = 350

        hrp.CFrame = CFrame.new(startPos)

        local distance = (endPos - startPos).Magnitude
        local time = distance / speed

        local tween = TweenService:Create(
            hrp,
            TweenInfo.new(time, Enum.EasingStyle.Linear),
            {CFrame = CFrame.new(endPos)}
        )

        tween:Play()
    end
})

