--- √ÇM THANH CLICK CH·ªà D√ôNG TRONG GUI

local SoundService = game:GetService("SoundService")

local clickSound = Instance.new("Sound")
clickSound.SoundId = "rbxassetid://70452176150315"
clickSound.Volume = 5
clickSound.Parent = SoundService

-- H√†m g·∫Øn sound cho to√†n b·ªô GUI
local function addClickSound(gui)
	for _, v in pairs(gui:GetDescendants()) end
		if v:IsA("AddButton") or v:IsA("AddToggle") then
			v.MouseButton1Click:Connect(function()
				clickSound:Play()
			end)
		end
	end
end

-- T·ª± ƒë·ªông √°p d·ª•ng cho t·∫•t c·∫£ GUI c·ªßa ng∆∞·ªùi ch∆°i
local player = game.Players.LocalPlayer
player.PlayerGui.ChildAdded:Connect(function(gui)
	task.wait(0.1)
	addClickSound(gui)
end)

-- √Åp d·ª•ng cho GUI ƒë√£ t·ªìn t·∫°i
for _, gui in pairs(player.PlayerGui:GetChildren()) do
	addClickSound(gui)
end
