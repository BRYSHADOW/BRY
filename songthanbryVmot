local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998"
})

Tab:AddButton({
    Name = "üöÄ Teleport ƒêi·ªÉm Radioactive",
    Callback = function()
        local player = game.Players.LocalPlayer
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")

        hrp.CFrame = CFrame.new(896.75, -19.36 + 3, -586.70)
    end
})

Tab:AddToggle({
    Name = "Auto TP Treo ‚è∞",
    Default = SaveData.AutoTPT,
    Callback = function(v)
        SaveData.AutoTPT = v
        Save()
        getgenv().AUTO_TP_TREO = v

        if not v then
            return
        end

        -- ===== CONFIG =====
        local Players = game:GetService("Players")
        local TeleportService = game:GetService("TeleportService")
        local lp = Players.LocalPlayer

        local TIMES = 15
        local DELAY = 0.5

        local POS1 = Vector3.new(425.26, -14.16, -340.05)
        local POS2 = Vector3.new(1131.63, 3.11, 531.83)
        local POS3 = Vector3.new(2571.70, -6.74, -336.66)

        -- ===== FUNC =====
        local function getHRP()
            local char = lp.Character or lp.CharacterAdded:Wait()
            return char:WaitForChild("HumanoidRootPart")
        end

        local function teleport(pos)
            getHRP().CFrame = CFrame.new(pos)
        end

        local function spamTP(pos)
            for i = 1, TIMES do
                if not getgenv().AUTO_TP_TREO then return end
                teleport(pos)
                task.wait(DELAY)
            end
        end

        local function inTime()
            local m = tonumber(os.date("%M"))
            return m >= 5 and m < 15
        end

        -- ===== AUTO TREO LOOP =====
        task.spawn(function()
            while getgenv().AUTO_TP_TREO do
                if inTime() then
                    spamTP(POS1)
                    spamTP(POS2)
                    spamTP(POS3)

                    if getgenv().AUTO_TP_TREO then
                        task.wait(1)
                        TeleportService:Teleport(game.PlaceId, lp)
                        break
                    end
                else
                    task.wait(10)
                end
            end
        end)
    end
})

