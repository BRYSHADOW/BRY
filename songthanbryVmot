local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998"
})

Tab:AddButton({
    Name = "üöÄ Teleport ƒêi·ªÉm Radioactive",
    Callback = function()
        local player = game.Players.LocalPlayer
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")

        hrp.CFrame = CFrame.new(896.75, -19.36 + 3, -586.70)
    end
})


Tab:AddToggle({
    Name = "Auto TP Treo Money ‚è∞",
    Default = SaveData.AutoTPT,
    Callback = function(v)
        SaveData.AutoTPT = v
        Save()
        getgenv().AUTO_TP_TREO = v

        if not v then
            return
        end

        -- ===== CONFIG =====
        local Players = game:GetService("Players")
        local TeleportService = game:GetService("TeleportService")
        local lp = Players.LocalPlayer

        local TIMES = 10
        local DELAY = 0.5

        local POS1 = Vector3.new(425.26, -14.16, -340.05)
        local POS2 = Vector3.new(1131.63, 3.11, 531.83)
        local POS3 = Vector3.new(2571.70, -6.74, -336.66)

        -- ===== FUNC =====
        local function getHRP()
            local char = lp.Character or lp.CharacterAdded:Wait()
            return char:WaitForChild("HumanoidRootPart")
        end

        local function teleport(pos)
            getHRP().CFrame = CFrame.new(pos)
        end

        local function spamTP(pos)
            for i = 1, TIMES do
                if not getgenv().AUTO_TP_TREO then return end
                teleport(pos)
                task.wait(DELAY)
            end
        end

        local function inTime()
            local m = tonumber(os.date("%M"))
            return m >= 5 and m < 15
        end

        -- ===== AUTO TREO LOOP =====
        task.spawn(function()
            while getgenv().AUTO_TP_TREO do
                if inTime() then
                    spamTP(POS1)
                    spamTP(POS2)
                    spamTP(POS3)

                    if getgenv().AUTO_TP_TREO then
                        task.wait(1)
                        TeleportService:Teleport(game.PlaceId, lp)
                        break
                    end
                else
                    task.wait(10)
                end
            end
        end)
    end
})

Tab:AddButton({
    Name = "Anti S·ªëng L√†m VƒÉng V√† Ng∆∞·ªùi ƒê√°nh (Fling)ü•Ä",
    Callback = function()
        task.spawn(function()
            while true do
                local rs = game:GetService("ReplicatedStorage")
                local re = rs:FindFirstChild("RemoteEvents")
                if re then
                    local fling = re:FindFirstChild("FlingSelf")
                    if fling then
                        fling:Destroy()
                    end
                end
                task.wait(0.25)
            end
        end)
    end
})

Tab:AddToggle({
    Name = "‚ú® Lucky ESP (Divine / Celestial)",
    Default = SaveData.LuckyESP,
    Callback = function(v)
        SaveData.LuckyESP = v
        Save()

        if v then
            loadstring([[
                if _G.HACKBORY_LUCKY_RUNNING then return end
                _G.HACKBORY_LUCKY_RUNNING = true

                local StarterGui = game:GetService("StarterGui")
                local RunService = game:GetService("RunService")

                local function rainbow()
                    return Color3.fromHSV((tick() % 5) / 5, 1, 1)
                end

                local function detectRarity(name)
                    name = name:lower()
                    if name:find("celestial") then
                        return "Celestial"
                    elseif name:find("divine") then
                        return "Divine"
                    end
                end

                local function getColor(rarity)
                    if rarity == "Divine" then
                        return Color3.fromRGB(255, 215, 0)
                    else
                        return Color3.fromRGB(255, 105, 180)
                    end
                end

                local notified = {}

                local function notifyDivine(blockName)
                    if notified[blockName] then return end
                    notified[blockName] = true

                    StarterGui:SetCore("SendNotification", {
                        Title = "‚ú® Divine Spawned!",
                        Text = blockName,
                        Duration = 4
                    })
                end

                local function createESP(block, rarity)
                    if block:FindFirstChild("LuckyESP") then return end

                    local mainPart = block:FindFirstChildWhichIsA("BasePart")
                    if not mainPart then return end

                    local gui = Instance.new("BillboardGui")
                    gui.Name = "LuckyESP"
                    gui.Size = UDim2.new(0, 180, 0, 30)
                    gui.AlwaysOnTop = true
                    gui.StudsOffset = Vector3.new(0, 3, 0)
                    gui.MaxDistance = 99999
                    gui.Parent = mainPart

                    local label = Instance.new("TextLabel")
                    label.BackgroundTransparency = 1
                    label.Size = UDim2.new(1, 0, 1, 0)
                    label.TextSize = 13
                    label.Font = Enum.Font.SourceSansBold
                    label.TextStrokeTransparency = 0.5
                    label.TextColor3 = getColor(rarity)
                    label.Text = "Lucky: " .. rarity
                    label.Parent = gui
                end

                local function createHighlight(model)
                    if model:FindFirstChild("LuckyHL") then return end

                    local hl = Instance.new("Highlight")
                    hl.Name = "LuckyHL"
                    hl.FillTransparency = 1
                    hl.OutlineTransparency = 0
                    hl.Parent = model

                    RunService.RenderStepped:Connect(function()
                        if hl.Parent then
                            hl.OutlineColor = rainbow()
                        end
                    end)
                end

                while _G.HACKBORY_LUCKY_RUNNING do
                    local folder = workspace:FindFirstChild("ActiveLuckyBlocks")

                    if folder then
                        for _, block in pairs(folder:GetChildren()) do
                            local rarity = detectRarity(block.Name)

                            if rarity then
                                createHighlight(block)
                                createESP(block, rarity)

                                if rarity == "Divine" then
                                    notifyDivine(block.Name)
                                end
                            end
                        end
                    end

                    task.wait(1)
                end
            ]])()
        else
            _G.HACKBORY_LUCKY_RUNNING = false

            for _, v in pairs(workspace:GetDescendants()) do
                if v.Name == "LuckyESP" or v.Name == "LuckyHL" then
                    v:Destroy()
                end
            end
        end
    end
})


Tab:AddToggle({
    Name = "‚ö° B·∫•m Nhanh",
    Default = SaveData.InstantPick,
    Callback = function(v)
        SaveData.InstantPick = v
        Save()

        if v then
            loadstring([[
                -- HACKBORY | Instant ProximityPrompt
                for _, p in pairs(workspace:GetDescendants()) do
                    if p:IsA("ProximityPrompt") then
                        p.HoldDuration = 0
                        p.RequiresLineOfSight = false
                    end
                end

                workspace.DescendantAdded:Connect(function(p)
                    if p:IsA("ProximityPrompt") then
                        p.HoldDuration = 0
                        p.RequiresLineOfSight = false
                    end
                end)
            ]])()
        end
    end
})

Tab:AddButton({
    Name = "üöÄ Teleport L√™n (Y√™u c·∫ßu Speed:400)",
    Callback = function()

        local TweenService = game:GetService("TweenService")
        local Players = game:GetService("Players")

        local plr = Players.LocalPlayer
        local char = plr.Character or plr.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")

        local startPos = Vector3.new(122.54, 140, 44.19)
        local endPos   = Vector3.new(4032.78, 140, -6.35)
        local finalPos = Vector3.new(4027.84, -2.86, -10.90)

        local speed = 550
        local dropSpeed = 300

        hrp.CFrame = CFrame.new(startPos)

        local distance = (endPos - startPos).Magnitude
        local time = distance / speed

        local tween = TweenService:Create(
            hrp,
            TweenInfo.new(time, Enum.EasingStyle.Linear),
            {CFrame = CFrame.new(endPos)}
        )

        tween:Play()

        tween.Completed:Connect(function(state)
            if state == Enum.PlaybackState.Completed then

                local dropDistance = (finalPos - endPos).Magnitude
                local dropTime = dropDistance / dropSpeed

                local dropTween = TweenService:Create(
                    hrp,
                    TweenInfo.new(dropTime, Enum.EasingStyle.Linear),
                    {CFrame = CFrame.new(finalPos)}
                )

                dropTween:Play()

                dropTween.Completed:Connect(function(dropState)
                    if dropState == Enum.PlaybackState.Completed then

                        local glass = Instance.new("Part")
                        glass.Size = Vector3.new(300, 5, 300)
                        glass.Anchored = true
                        glass.Material = Enum.Material.Glass
                        glass.Transparency = 0.5
                        glass.Color = Color3.fromRGB(255, 0, 0)
                        glass.CanCollide = false

                        glass.CFrame = hrp.CFrame * CFrame.new(0, 3, 0)
                        glass.Parent = workspace

                        task.delay(3, function()
                            if glass then
                                glass:Destroy()
                            end
                        end)
                    end
                end)
            end
        end)

    end
})

